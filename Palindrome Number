class Solution {
    public boolean isPalindrome(int x) {
        
    int num = x,rev = 0 ;
    int temp = num;


    while(num>0){
        int digit = num%10;
        rev = rev*10 + digit;
        num = num/10;
    }
    
    if (temp==rev)
    return true;
    else 
    return false;

        
    }
}
